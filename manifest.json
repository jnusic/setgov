{
  "manifest_version": 2,

  "name": "SetGov",
  "description": "A local election platform",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.min.js; object-src 'self'",

  "browser_action": {
    "default_icon": {                    
      "19": "logo_no_circle.png"          
    },
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "background": { "scripts": ["popup.js"] },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js": ["getKeyWordsFromPage.js"],
       "run_at": "document_end"
    }
  ],
   "permissions": [
    "tabs","<all_urls>"
  ]
}